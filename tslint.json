{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "cliOptions": {
    "exclude": ["**/generated/**"]
  },
  "linterOptions": {
    "exclude": ["**/generated/**"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "ordered-imports": false,
    "no-submodule-imports": false,
    "array-type": false,
    "indent": [true, "tabs", 2],
    "no-console": [false, "error"]
  }
}
